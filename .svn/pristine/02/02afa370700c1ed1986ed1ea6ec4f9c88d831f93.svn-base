body {
	background-color: #EAEEF0;
}

.navbar-fixed-top {
    box-shadow: 0px 0px 6px #7C7E80;
}

.ui-widget-header {
    background: #1D3445;
    color: #fff;
}

.dlb_filtro {
	font-size: 13px !important;
    border: 1px solid #1d3445;
    border-radius: 3px;
}

#info-usuario {
    display: none;
}

@media (min-width: 800px) {
	#info-usuario {
		display: block;
	}
}

#info-usuario {
	font-weight: bold;
    margin: 10px 0 0 15px;
    font-size: 11px;
}

#info-usuario p {
	margin-bottom: 0;
}

#selo {
	float: left;
	padding: 4px 5px 3px 10px;
}

#logo {
	width: 170px;
    margin-top: 7px;
}

#rodape {
	width: 100%;
	height: 40px;
	font-size: 11px;
	text-align: center;
	position: absolute;
	bottom: 0;
}

.section {
	display: none;
}

/** dialog geral **/
.dlg-geral .ui-button {
    padding: .3em 0.5em;
    font-size: 13px;
} 
.dlg-geral .ui-dialog-buttonpane {
	padding: 0 1em 0 .4em;
}
.dlg-geral .ui-widget-content .panel-title a {
    color: inherit;
}
.dlg-geral .ui-dialog-content {
	font-size: 12px;
}
/** /dialog geral **/

/** dialog confirmacao **/
.dlg-confirm .ui-button {
    padding: .3em 0.5em;
    font-size: 13px;
    font-weight: bold;
}

.dlg-confirm .ui-dialog-buttonpane {
	padding: 0 1em 0 .4em;
}

.dlg-confirm .confirm-warning {
	font-size: 25px;
    margin-top: 8px;
}

#dlg-confirm-msg, #dlg-alert-msg {
	font-size: 13px;
}
/** /dialog confirmacao **/

.fp-tableCell {
    font-size: 12px;
}

@media (min-width: 800px) {
	.fp-tableCell {
	    vertical-align: top;
	    padding-top: 75px;
	    padding-left: 150px;
	    padding-right: 150px;
	    display: block;
	}
	#section-agend .fp-tableCell {
	    padding-left: 50px;
	    padding-right: 50px;
	}
}

@media (min-width: 1500px) {
	#section-agend .fp-tableCell {
	    padding-left: 150px;
	    padding-right: 150px;
	}
}

.fp-controlArrow.fp-prev {
    border-width: 20.5px 24px 20.5px 0;
    border-color: transparent #1d3445 transparent transparent;
}

.fp-controlArrow.fp-next {
    border-width: 20.5px 0 20.5px 24px;
    border-color: transparent transparent transparent #1d3445;
}

.fp-controlArrow {
	display: none;	
}

/** btn **/
.btn {
    color: #fff;
    /*background-color: #2f4e65;
    border-color: #1d3445;*/
    background-color: #43637b;
    border-color: #2f4e65;
    transition: all 0.3s ease 0s;
}

.btn:hover, .btn:active, .btn:focus {
    color: #1d3445;
    /*background-color: #fff;*/
    background-color: #c5d3dc;
    border-color: #1d3445;
}

.btn.active {
	color: #1d3445;
    background-color: #c5d3dc;
}
/** /btn **/

/** form **/
.form-control {
	border-color: #1d3445;
}
.input-group-addon {
    background-color: #1d3445;
    border: 1px solid #1d3445;
    color: #fdfdfd;
}
.periodo-modif {
	margin-bottom: 2px;
}
.tipo-periodo {
	width: auto;
    display: inline-block;
}
/** /form **/

/** abas **/
.nav-tabs li a {
    font-size: 12px;
}
.nav-tabs>li.active>a, .nav-tabs>li.active>a:focus, .nav-tabs>li.active>a:hover {
    border: 1px solid #1d3445;
}
.tab-pane {
    border: 1px solid #1d3445;
    border-top: none;
}
.tab-pane-vertical {
    border-top: 1px solid #1d3445;
}
.nav-tabs {
    border-bottom: 1px solid #1d3445;
}
.nav-tabs li a {
    border: 1px solid #1d3445;
    border-bottom: none;
}
.nav-tabs li a:hover {
    border-color: #1d3445;
    color: #000;
}
/** /abas **/

.btn-fechar-aba {
	position: absolute;
    top: 2px;
    right: 2px;
    cursor: pointer;
}

/** table **/
.table {
	border-radius: 3px;
	border-color: #1d3445;
}

.table .group {
	font-weight: bold;
    background-color: #1d3445 !important;
    color: #fdfdfd;
    cursor: pointer;
}

.table .group td {
	padding-left: 15px;
}

.pagination>.active>a, .pagination>.active>a:focus, .pagination>.active>a:hover, .pagination>.active>span, .pagination>.active>span:focus, .pagination>.active>span:hover {
    background-color: #1d3445;
    border-color: #1d3445;
}

.dataTables_info {
	font-size: 12px;
    font-style: italic;
}

.table th, .table td {
	white-space: nowrap;
}
/** /table **/

/** panel **/
.panel {
	margin-bottom: 0;
}

.panel-heading {
    padding: 6px 15px;
}

.panel-title {
    font-size: 13px;
    font-weight: bold;
}

.panel-title a:focus, .panel-title a:hover {
    text-decoration: none;
}

.panel-group .panel+.panel {
    margin-top: 1px;
}

.panel-default {
    border-color: #1d3445;
}

.panel-default>.panel-heading {
    color: #fdfdfd;
    background-color: #1d3445;
    border-color: #1d3445;
}
/** /panel **/

/** relatorios **/

#relatorios div.dataTables_wrapper div.dataTables_filter {
    text-align: left;
}

#relatorios .panel-body {
    padding: 10px 15px 5px;
}

.btn-rel-agend {
	text-align: center;
	margin-bottom: 20px;
	font-weight: normal;
}

.btn-rel-agend .btn.active {
	font-weight: bold;
}

#relatorios input[type=checkbox] {
	cursor: pointer;
	margin: 0;
}

#relatorios .acoes {
	text-align: right;
}

#relatorios .acoes button {
	margin-right: 5px;
}

/** /relatorios **/

/** configuracao **/
#configuracao {
	display: none;
}

#configuracao legend {
	font-size: 13px;
    border-bottom: 1px solid #ddd;
    margin-bottom: 10px;
    font-weight: bold;
}

.conf-row {
	margin: 15px 5px;
    color: #fff;
    background: #1d3445;
    border-radius: 4px;
}

.conf-row.edit {
    z-index: 100;
    position: relative;
    box-shadow: 0px 4px 10px #666;
}

.conf-row input, .conf-row select {
	color: #000;
    line-height: initial;
    width: 200px;
    padding: 5px;
    border-radius: 2px;
    border: 0;
}

.conf-row .conf-row-btn {
	margin-top: 5px;
}

.conf-row .col-xs-4 {
    font-weight: bold;
    line-height: 40px;
}

.wizard > .content {
	min-height: 430px;
}

@media (min-height: 800px) {
	.wizard > .content {
	    min-height: 530px;
	}
}

.wizard > .content > .body input {
    display: inherit;
    border-color: #1d3445;
}

.wizard > .content > .body {
	width: 100%;
	height: 100%;
}

.wizard > .steps a {
    text-align: center;
    background: #1d3445;
    color: #fff;
    font-weight: bold;
}

.wizard > .steps .current a, .wizard > .steps .current a:hover, .wizard > .steps .current a:active {
    background: #c5d3dc !important;
    color: #1d3445;
    box-shadow: 0px 2px 6px #1d3445;
}

.wizard > .steps .done a, .wizard > .steps .done a:hover, .wizard > .steps .done a:active {
    background: #1d3445;
}

.wizard > .actions a, .wizard > .actions a:hover, .wizard > .actions a:active {
	background: #1d3445;
}
/** /configuracao **/

/** CALENDARIO **/ 
.datepicker-inline {
display: block !important;
}

.datepicker {
padding: 4px;
border-radius: 4px;
border: 2px solid #9AA9B7;
background-color: #FFFFFF;
cursor: default;
}

.datepicker table {
background-color: #FFFFFF;
}

.datepicker tr {
font-size: 11px;
}

.datepicker td, .datepicker th {
width: 25px !important;
height: 25px !important;
}

.datepicker th {
font-size: 12px;
font-weight: bold;
color: #08071D;
}

.datepicker .datepicker-switch {
background-color: #9AA9B7;
}

.datepicker .datepicker-switch:hover {
background-color: #6D7B88 !important;
}

.datepicker .cw{
font-size: 11px;
padding: 0 9px;
font-weight: bold;
color: #080706;
}

.datepicker table tr td.day{
color: #08071D;
font-weight: bold;
}

.datepicker table tr td.day:hover, .datepicker table tr td.day.focused {
background: #DBE3EA;
cursor: pointer;
}

.datepicker table tr td.active.active, .datepicker table tr td.active:hover.active{
background: #9AA9B7;
cursor: pointer;
color: #FFFFFF;
}

.datepicker table tr td.disabled, .datepicker table tr td.disabled:hover {
background: 0 0;
color: #C3BAAC;
cursor: default;
font-weight: normal;
} 

.datepicker table tr td.old, .datepicker table tr td.new{
color: #7E746C;
}
/** /CALENDARIO **/

/** periodo **/
.duracao, .bootstrap-touchspin {
	width: 42px !important;
}
.bootstrap-touchspin .input-group-btn-vertical > .btn {
    padding: 7px 9px;
}
.legenda-duracao {
    font-size: 11px;
    font-weight: bold;
    text-align: center;
}
.intervalos-especificos {
    border: 1px solid #1d3445;
    border-radius: 2px;
    background-color: #fff;
}
.intervalos-especificos ul, .intervalos-especificos-resumo ul {
    height: 110px;
    overflow: auto;
}
.intervalos-especificos li, .intervalos-especificos-resumo li {
	display: block !important;
    padding: 2px 5px !important;
}
.intervalos-especificos-cabecalho {
	border-bottom: 1px solid #1d3445;
    padding: 2px 5px;
}
.resumo-intervalos-pre {
    margin-bottom: 10px;
}
.resumo-periodo {
	background-color: #fff;
    padding: 5px;
    border: 1px solid #1d3445;
    border-radius: 4px;
    height: 330px;
	overflow: auto;
}
@media (min-height: 800px) {
	.resumo-periodo {
		height: 430px;
	}	
}
/** /periodo **/

/** colunas **/
.tree-colunas {
    height: 359px;
    overflow: auto;
	border: 1px solid #1d3445;
	border-radius: 3px;
}
.tree-colunas .list-group {
    background-color: #fff;
	margin-bottom: 0;
}
.tree-colunas .list-group-item {
    padding: 3px 10px !important;
    border: none;
    display: block !important;
}
.colunas-selecionadas, .colunas-selecionadas-resumo {
	height: 330px;
	padding: 0;
	overflow: auto;
}
@media (min-height: 800px) {
	.tree-colunas {
    	height: 459px;
	}
	.colunas-selecionadas, .colunas-selecionadas-resumo {
		height: 430px;
	}	
}
.colunas-selecionadas li {
	display: block !important;
    padding: 3px 5px !important;
    cursor: move;
    cursor: grab;
    cursor: -moz-grab;
    cursor: -webkit-grab;
}
.colunas-selecionadas-resumo li {
	display: block !important;
    padding: 2px 5px !important;
}
.colunas-selecionadas-resumo .alert-erros {
	margin: 5px;
}
.colunas-selecionadas li.grabbing {
	cursor: grabbing;
    cursor: -moz-grabbing;
    cursor: -webkit-grabbing;
}
.tree-colunas .badge {
	padding: 1px 4px;
    font-size: 10px;
}
/** /colunas **/

/** filtros **/
.wizard > .content > .body ul > li {
    display: block !important;
}

.section-filtros .nav-tabs>li.active>a, 
.section-filtros .nav-tabs>li.active>a:focus, 
.section-filtros .nav-tabs>li.active>a:hover {
    border-bottom: transparent !important;
    text-decoration: underline;
}

.tab-filtros-conteudos .tab-pane {
	height: 330px;
	overflow: auto;
    padding: 15px;
}

@media (min-height: 800px) {
	.tab-filtros-conteudos .tab-pane {
		height: 430px;
	}
}

.tab-filtros-conteudos input[type="checkbox"] {
	cursor: pointer;
	margin: 9px 0 0;
}

.tab-filtros-conteudos .campo-numerico {
	width: 100px !important;
}

.filtros-primarios {
    background-color: #c5d3dc !important;
}

.tab-filtros-conteudos .dlb_select {
	max-width: 200px;
}
/** /filtros **/
.filtros-tree {
    height: 250px;
    overflow: auto;
}

.filtros-tree .list-group {
    font-size: 12px;
	margin-bottom: 0;
}

.filtros-tree .list-group-item {
    padding: 3px 10px !important;
    border: none;
    display: block !important;
}

.dlg-filtros-tree .ui-dialog-content {
    padding: 5px 1px;
}

.dlg-filtros-tree .ui-button {
    padding: .3em 0.5em;
    font-size: 13px;
    font-weight: bold;
}

.dlg-filtros-tree .ui-dialog-buttonpane {
	padding: 0 1em 0 .4em;
}

.dlg-filtros-tree form {
	padding: 5px;
    margin-bottom: 5px;
    border: 1px solid #ddd;
    background: #e9e9e9;
    border-radius: 3px;
}

.fp-selec {
    font-weight: bold;
    margin-left: 10px;
}

.filtros-tree-action-bar button, .filtros-tree-action-bar .btn-group {
	margin-left: 5px;
}

.filtros-tree-action-bar-edit {
	margin-left: 10px;
	display: none;
}

.filtros-tree-action-bar-ext {
    margin-top: 3px;
}

.filtros-tree-upload {
	font-size: 12px;
	display: none;
}

.filtros-tree-upload .fileUpload {
	margin-left: 15px;
}

.resumo-filtros-sel {
	background-color: #fff;
    padding: 5px;
    border: 1px solid #1d3445;
    border-radius: 4px;
    height: 330px;
	overflow: auto;
}
.resumo-filtros-sel fieldset {
	margin-bottom: 10px;
}
@media (min-height: 800px) {
	.resumo-filtros-sel {
		height: 430px;
	}	
}

/** upload **/
.fileUpload {
    position: relative;
    overflow: hidden;
}
.fileUpload input.upload {
    position: absolute;
    top: 0;
    right: 0;
    margin: 0;
    padding: 0;
    font-size: 20px;
    cursor: pointer;
    opacity: 0;
    filter: alpha(opacity=0);
}
/** /upload **/

/** execucao relatorio **/
.execucao-relatorio {
	z-index: 1031;
}

.execucao-relatorio .ui-dialog-titlebar {
    background-repeat: no-repeat;
    background-position: 3px 8px;
    padding-left: 25px;
    color: #333;
}

.execucao-relatorio .ui-dialog-titlebar.executando {
	background-image: url(../images/loading.gif);
	background-color: #FCF8E3;
}

.execucao-relatorio .ui-dialog-titlebar.pronto {
	background-image: url(../images/ok.png);
	background-color: #DFF0D8;
	background-position: 5px 10px;
}

.execucao-relatorio .ui-dialog-titlebar.erro {
	background-image: url(../images/erro.png);
	background-color: #F2DEDE;
}

.dlg-execucao {
	padding: 5px 20px !important;
	background-color: #EAEEF0 !important;
    border: 1px solid #ddd !important;
    border-radius: 3px;
    margin-top: 3px;
}

.dlg-execucao .table {
	width: 100% !important;
}

.execucao-barra {
    border: 1px solid #ddd;
    background: #fff;
    padding: 5px;
    border-radius: 4px;
    font-size: 12px;
    margin-bottom: 10px;
    margin-left: 0;
    margin-right: 0;
}

.btn-drill-down {
	display: none;
}

.table.drill tbody td.drill {
	cursor: pointer;
	background-color: #fffcdc;
}

.table.drill tbody td.drill:hover {
	background-color: #eceacd;
}

#dlg-colunas-drill {
	font-size: 11px;
}

#dlg-colunas-drill .treeview span.indent {
    margin-left: 10px;
    margin-right: 0;
}
/** /execucao relatorio **/

/** compartilhamento **/
#dlg-compartilhamento .dlb_select {
    min-width: 200px;
    font-size: 12px !important;
    min-height: 255px;
}

.dlg-compartilhamento .ui-dialog-buttonpane {
	padding: 0 1em 0 .4em;
}

.dlg-compartilhamento .dlb_col_central {
    margin-top: 90px;
}

.dlg-compartilhamento .ui-button {
    padding: .3em 0.5em;
    font-size: 13px;
}
/** /compartilhamento **/

/** agendamento **/
#agendamento {
    /*background-color: #fff;
    padding: 10px 0;
    border: 1px solid #1d3445;
    border-radius: 4px;*/
}

.ag-exportacao {
	margin-top: 15px;
    border: 1px solid #ccc;
    border-radius: 4px;
    padding: 5px 10px;
    font-size: 11px;
}

.ag-exportacao h5 {
    margin: 2px 0 2px 0;
    font-weight: bold;
    font-size: 12px;
}

.ag-periodicidade-conf {
	display: none;
}

#agendamento .checkbox label, #agendamento .radio label {
	margin-right: 10px;
}

#agendamento .dlb_select {
    min-width: 170px;
}

.ag-conf-relatorios .dlb_select {
    font-size: 12px !important;
    min-height: 255px;
}

.ag-conf-relatorios .dlb_col_central {
    margin-top: 90px;
}

#dlg-ag-execs {
	padding: 10px 15px;
}

.latencia {
	width: 35px !important;
}

.ag-hora-consolidacao {
	border-left: 1px dashed;
	font-size: 11px;
}

.ag-col-conf {
	max-height: 450px;
    overflow: auto;
}

.col-acoes {
    width: 70px;
}
/** /agendamento **/

/** resumo **/
.section-resumo {
	padding-top: 10px !important;
}
.section-resumo .alert {
	display: none;
    padding: 7px;
    margin-bottom: 5px;
}
/** /resumo **/